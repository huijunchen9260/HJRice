/* URxvt Setting: */

URxvt.intensityStyles:	true
URxvt.background:       [90]#2f2f2f
URxvt.depth:		32
/* URxvt.font: 		xft:monospace:size=20 */
URxvt.font: 		xft:ibm plex mono:size=20,\
			xft:DejaVu Sans Mono:size=20,\
			xft:IPAGothic:size=20,\
			xft:FreeMono:size=20,\
			xft:FreeSans:size=20,\
			xft:Symbola:size=20,\
			xft:Joypixels:size=9,\
			-*-unifont-*-*-*-*-*-*-*-*-*-*-*-*
/* URxvt.letterspace:	-1 */
URxvt.scrollBar:	false
URxvt.cursorColor:	#dcdccc

!! Extensions
URxvt.perl-ext-common:	default,clipboard,matcher,resize-font,url-select,keyboard-select,
URxvt.colorUL:		#add8e6
!! url-select
!! Ctrl-u to enter url-select mode
!! j to previous url, k to next url.
!! Return to open url, o to open url and stay in mode
!! y to copy url, Esc to leave mode
URxvt.keysym.C-Mod4-u:	perl:url-select:select_next
URxvt.url-select.launcher: linkhandler
/* URxvt.url-select.launcher: /usr/bin/xdg-open */
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.S-Escape: perl:keyboard-select:activate
!! resize-font
!! Enable Ctrl-Shift bindings
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.C-Mod4-j:     resize-font:smaller
URxvt.keysym.C-Mod4-k:      resize-font:bigger
URxvt.keysym.C-Mod4-equal:     resize-font:reset
URxvt.keysym.C-Mod4-question:  resize-font:show

!! Matcher
URxvt.url-launcher:	linkhandler
URxvt.matcher.button:	1
!! clipboard
!! Ctrl-y to copy, Ctrl-p to paste
URxvt.clipboard.autocopy: true
URxvt.keysym.C-y: perl:clipboard:copy
URxvt.keysym.C-p: perl:clipboard:paste

Sxiv.foreground: #bbbbbb
Sxiv.background: #222222

Xcursor.theme: Neutral
Xcursor.size: 48
